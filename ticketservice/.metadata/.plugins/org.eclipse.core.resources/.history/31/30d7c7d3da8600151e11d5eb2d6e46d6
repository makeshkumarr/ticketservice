package org.test.ticketservice.vo;

import java.io.Serializable;
import java.util.List;

public class SeatHold implements Serializable{

	private static final long serialVersionUID = 3888840549041404679L;

	private String seatHoldId;
	private List<LevelRowSeat> heldSeats;
	
	public SeatHold(String seatHoldId, List<LevelRowSeat> heldSeats) {
		this.seatHoldId = seatHoldId;
		this.heldSeats = heldSeats;
	}

	public String getSeatHoldId() {
		return seatHoldId;
	}

	public List<LevelRowSeat> getHeldSeats() {
		return heldSeats;
	}
}
